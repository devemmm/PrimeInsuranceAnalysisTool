        <div class="col-lg-6">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Very Satisfied</h5>


              <!-- Pie Chart -->
              <div id="verry_satisfied"></div>
              <script>
                document.addEventListener("DOMContentLoaded", async() => {
                  const url = 'https://prime-insurance-api.nextreflexe.com/admin/survey/statistics';
                  
                  let series = []
                  let labels = []
                  try{
                    const value = await fetch(url, {
                      method: 'GET',
                      headers: {
                        Accept: 'application/json',
                        'Content-Type': 'application/json',
                      },
                    });
                    const response = await value.json();

                    const data = response.statistics.very_satisfied

                    data.forEach(se=>{
                      series.push(se.total)
                      labels.push(se.service)
                    })
                    console.log(series, labels)
                  }catch(error){
                    series = [1]
                    labels= ['EMPTY']
                    console.log(error)
                  }

                  new ApexCharts(document.querySelector("#verry_satisfied"), {
                    series ,
                    chart: {
                      height: 350,
                      type: 'pie',
                      toolbar: {
                        show: true
                      }
                    },
                    labels
                  }).render();
                });
              </script>
              <!-- End Pie Chart -->

